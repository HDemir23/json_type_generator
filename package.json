{
  "name": "json-type-gen",
  "version": "1.0.0",
  "description": "A CLI tool that generates type definitions from JSON files for multiple programming languages",
  "main": "main.ts",
  "bin": {
    "json-type-gen": "./main.ts"
  },
  "scripts": {
    "start": "ts-node main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "example": "ts-node main.ts sample.json ts --dry-run",
    "example:go": "ts-node main.ts sample.json go --sample 5 --strict",
    "example:rust": "ts-node main.ts sample.json rust --output types.rs --backup"
  },
  "keywords": [
    "json",
    "types",
    "generator",
    "cli",
    "typescript",
    "go",
    "rust",
    "c",
    "c++",
    "c#",
    "swift",
    "django"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
} 